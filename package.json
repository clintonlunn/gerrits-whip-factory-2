{
    "title": "Gerrit's Whip Factory",
    "name": "startbootstrap-agency",
    "version": "6.0.3",
    "scripts": {
        "build": "npm run build:html && npm run build:cname && npm run clean && npm run build:scss && npm run build:scripts && npm run build:assets && npm run build:replaceHtml && npm run build:replaceCname",
        "build:html": "node scripts/build-html.js",
        "build:cname": "node scripts/build-cname.js",
        "build:replaceCname": "node scripts/replace-cname.js",
        "build:assets": "node scripts/build-assets.js",
        "build:pug": "node scripts/build-pug.js",
        "build:scripts": "node scripts/build-scripts.js",
        "build:scss": "node scripts/build-scss.js",
        "build:replaceHtml": "node scripts/replace-html.js",
        "clean": "node scripts/clean.js",
        "start": "npm run build && node scripts/start.js",
        "start:debug": "npm run build && node scripts/start-debug.js",
        "deploy": "gh-pages -d dist"
    },
    "description": "Agency is a one page HTML theme for Bootstrap.",
    "keywords": [
        "css",
        "sass",
        "html",
        "responsive",
        "theme",
        "template"
    ],
    "homepage": "https://gerrritswhipfactory.com/",
    "bugs": {
        "url": "https://github.com/gerritbutler/gerrits-whip-factory/issues",
        "email": "gerrit@gerritswhipfactory.com"
    },
    "license": "MIT",
    "author": "Start Bootstrap",
    "contributors": [
        "Clinton Lunn (https://clintonlunn.com/)",
        "Gerrit Butler"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/gerritbutler/gerrits-whip-factory.git"
    },
    "dependencies": {
        "bootstrap": "4.5.3"
    },
    "devDependencies": {
        "autoprefixer": "10.0.1",
        "browser-sync": "2.26.13",
        "chokidar": "3.4.3",
        "concurrently": "5.3.0",
        "gh-pages": "^3.1.0",
        "postcss": "8.4.31",
        "prettier": "2.1.2",
        "pug": "3.0.0",
        "sass": "1.28.0",
        "shelljs": "0.8.4",
        "upath": "^2.0.1"
    }
}
